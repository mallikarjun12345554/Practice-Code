package com.five.Hybrid;

public class HumanBody extends Object{
	public static final String  = null;
	public void displayHuman() {
		System.out.println("Method defined inside HumanBody class");
	
}

interface Male
{
public void show();
}
interface Female
{
public void show();
}
public class Child extends HumanBody implements Male, Female{
	public void show()
	{
		System.out.println("Implementation of show()"
				+ "method in the interfaces of Male, Female ");
	}
public void displayChild() {
	
	System.out.println("Method inside the Child class");
}

public static void main(String[] args, Object HumanBody) {

	System.out.println("Implementation of Hybrid Inheritance in java");
	obj.show();
	obj.displayChild();
	

}	
}}

