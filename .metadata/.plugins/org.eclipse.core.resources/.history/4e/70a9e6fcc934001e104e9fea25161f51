package com.explicit;

public class Eg3 {

	public static void main(String[] args) {
            double d = 10.0d;
            System.out.println(d);
            
            float f = (float)d;
            System.out.println(f);
            
            long l = (long)f;
            System.out.println(l);
            
            int i = (int)l;
            System.out.println(i);
            		
	}

}
