package com.explicit;

public class Eg2 {

	public static void main(String[] args) {
              long  l = 90;
              System.out.println(l);
              
              
              char ch = (char)l;
              System.out.println(ch);
              
              float f2 = 20.0f;
              long l2 = (long)f2;
               System.out.println(l2);
               
               
               
               int i = 65;
               char ch3 = (char)i;
               System.out.println(ch3);
               
               
              
              
	}

}
